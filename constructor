
 

 
 

class Constructo{
    int number ;
     String name;

    // default consteructor 
     Constructo() {
        System.out.println("it is default ");
    }

    // perameter 

     Constructo(int number ,String name){
        this.number=number;
        this.name= name;
      
     }

    void person(){
        System.out.println("numberr is the "+number);
        System.out.println("name is thee "+name);
     }
    
}

class construter{
    public static void main(String[] args) {
        
    
    Constructo myc=new Constructo(5,"deepak");
    Constructo my=new Constructo();
    myc.person();
    my.person();

    }



}
   

     
            
            
  

